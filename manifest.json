{
	"browser_action": {
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"options_page": "options.html",
	"permissions": ["storage"],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"name": "Dog on Demand",
	"version": "1.0",
	"description": "Get a dog pic whenever you want!",
	"manifest_version": 2
}
